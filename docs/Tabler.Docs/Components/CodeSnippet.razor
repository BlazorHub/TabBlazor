<TablerCard>
    <TablerCardHeader>
        <h4 class="card-title">@Title</h4>
    </TablerCardHeader>
    <div class="p-3">
        @ChildContent
    </div>
    <div class="card-code">
        <figure class="highlight">
            @((MarkupString)@Code)
        </figure>
    </div>

</TablerCard>

