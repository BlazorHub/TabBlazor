@namespace Tabler.Components

<svg xmlns="http://www.w3.org/2000/svg"
     @attributes="Attributes"
     color="@Color"
     width="@Size"
     height="@Size"
     viewBox="0 0 24 24"
     stroke-width="@StrokeWidth"
     stroke="currentColor"
     fill="none"
     stroke-linecap="round"
     stroke-linejoin="round">
    @if (Elements != null)
    {
        @((MarkupString)Elements)
    }
</svg>